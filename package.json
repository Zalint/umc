{
  "name": "gambia-election-results",
  "version": "1.0.0",
  "description": "Election results collection system for The Gambia",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "restart": "taskkill /F /IM node.exe 2>nul & timeout /t 2 /nobreak >nul & npm start",
    "stop": "taskkill /F /IM node.exe",
    "db:schema": "psql $DATABASE_URL -f db/schema.sql",
    "db:seed": "node db/seed.js",
    "db:seed-prod": "node db/seed-users-prod.js",
    "db:set-passwords": "node db/set-prod-passwords.js",
    "db:import-voters": "node db/import-registered-voters.js",
    "db:update-passwords": "node db/update-passwords.js",
    "db:add-sessions": "node db/add-session-tracking.js",
    "db:add-comments": "node db/add-station-comments.js",
    "test:fill": "node db/fill-fake-data.js",
    "test:clear": "node db/clear-fake-data.js",
    "test:fill-pvt": "node db/fill-pvt-fake-data.js",
    "test:clear-pvt": "node db/clear-pvt-fake-data.js"
  },
  "keywords": ["election", "gambia", "results"],
  "author": "Unite Movement Gambia",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}

